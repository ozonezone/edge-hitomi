{
  "compilerOptions": {
	"target": "esnext",
	"module": "esnext",
	"moduleResolution": "node",
	"outDir": "./dist/",
	"strict": true,
	"esModuleInterop": true,
	"forceConsistentCasingInFileNames": true
  },
  "lib": [
	"es2024",
	"webworker",
	"dom"
  ],
  "include": [
	"./src/**/*",
	"./dist/index.d.ts"
  ],
  "exclude": [
	"./node_modules/",
	"./src/test.mts",
	"./dist/"
  ]
}
